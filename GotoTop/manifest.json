{
    "manifest_version": 3,
    "name": "go to top",
    "description": "It will lead you to top",
    "version": "1.0",
    "permissions": [
                    "scripting",
                    "activeTab",
                    "tabs"
                ],
    "action": {
      "default_popup": "popup/index.html",
      "default_icon": "images/arrow.png"
    },
    "commands": {
      "_execute_action":{
        "suggested_key": {
          "default": "Alt+Q",
          "mac": "Alt+Q"
        },
        "description": "Activate extention"
      },
      "up": {
        "suggested_key": {
          "default": "Alt+A",
          "mac": "Alt+A"
        },
        "description": "fly awaaaaaaay"
      },
      "down": {
        "suggested_key": {
          "default": "Alt+S",
          "mac": "Alt+S"
        },
        "description": "go doooooooown"
      }
    }
  }