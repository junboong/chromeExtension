{
    "manifest_version": 3,
    "name": "Now you in heaven",
    "description": "It will lead you to heaven",
    "version": "1.0",
    "permissions": [
                    "scripting",
                    "activeTab",
                    "tabs"
                ],
    "action": {
      "default_popup": "popup/index.html",
      "default_icon": "images/arrow.png"
    },
    "commands": {
      "_execute_action":{
        "suggested_key": {
          "default": "Alt+Q",
          "mac": "Alt+Q"
        },
        "description": "Activate extention"
      },
      "my_command": {
        "suggested_key": {
          "default": "Alt+W",
          "mac": "Alt+W"
        },
        "description": "Go to the top of page"
      }
    }
  }